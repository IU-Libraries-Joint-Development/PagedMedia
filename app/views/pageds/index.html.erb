<div id="sidebar" class="span3">
  <%= render partial: 'shared/side_navbar' %>
</div>
 
<% i = 0 %>
 
<div id="content" class="span9">
  <%= render_breadcrumbs %>
  
  <table id='listPageds'>
    <tbody>
      
      <% @pageds.each do |paged| %>  
      
        <tr style="font-weight: bold;">
          <td><%= i += 1 %>.</td>
          <td><%= link_to paged.title, paged %></td>
          <td rowspan=6>
            <%= image_tag(@image_urls[paged.pid], height: "100", width: "100" ) %>
          </td>
        </tr>
        <tr>
          <td>Creator:</td>
          <td><%= paged.creator %></td>
        </tr>
        <tr>
          <td>Publisher:</td>
          <td><%= paged.publisher %></td>
        </tr>
        <tr>
          <td>Publisher Place:</td>
          <td><%= paged.publisher_place %></td>
        </tr>
        <tr>
          <td>Type:</td>
          <td><%= paged.type %></td>
        </tr>
        <tr>
          <td>Issued:</td>
          <td><%= paged.issued %></td>
        </tr>
        <tr>

        </tr>

      <% end %>
    </tbody>
  </table>
 
  <br>
 
  <!-- Removed: No longer needed on the browse page
  <%= button_to 'New Paged', new_paged_path, :class=> "button", :method=> :get %>
  -->
</div>